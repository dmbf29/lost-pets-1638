<div class="container">
  <div class="d-flex justify-content-between">
    <h1>Found Pets</h1>
    <%= link_to 'Add Pet', new_pet_path, class: 'btn btn-success'  %>
  </div>
  <% @pets.each do |pet| %>
    <h3><%= link_to pet.name, pet_path(pet), class: 'text-decoration-none' %></h3>
  <% end %>
</div>
